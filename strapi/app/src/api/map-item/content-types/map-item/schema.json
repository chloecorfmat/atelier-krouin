{
  "kind": "collectionType",
  "collectionName": "map_items",
  "info": {
    "singularName": "map-item",
    "pluralName": "map-items",
    "displayName": "Map item",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "coordinates": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "website": {
      "type": "string",
      "regex": "^(?:http(s)?:\\/\\/)?[\\w.-]+(?:\\.[\\w\\.-]+)+[\\w\\-\\._~:/?#[\\]@!\\$&'\\(\\)\\*\\+,;=.]+$"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "bar",
        "restaurant",
        "point of view",
        "visit"
      ]
    }
  }
}
