{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "header": {
      "type": "text",
      "required": true
    },
    "slug": {
      "type": "string",
      "unique": true
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "block.cta-block",
        "block.paragraph-block"
      ],
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "articles"
    },
    "image_header": {
      "type": "component",
      "repeatable": false,
      "component": "element.image",
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo",
      "required": true
    },
    "linked_articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "inversedBy": "referenced_articles"
    },
    "referenced_articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "inversedBy": "linked_articles"
    },
    "unlisted": {
      "type": "boolean",
      "default": false
    }
  }
}
