{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "Articles",
    "description": "",
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Articles",
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "header": {
      "type": "text",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "component",
      "repeatable": true,
      "component": "content.article",
      "required": true
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::articles.articles"
    },
    "image_header": {
      "type": "component",
      "repeatable": false,
      "required": true,
      "component": "sub-component.image"
    },
    "categories": {
      "type": "repation",
      "relation": "oneToMany",
      "target": "api::categorie.categorie"
    },
    "instagram": {
      "type": "component",
      "repeatable": false,
      "component": "component.instagram-block"
    },
    "SEO": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo",
      "required": true
    }
  }
}
